ePath":"ChunJun连接器/gbase/gbase-source.md","ctime":"2022-06-28T12:20:34.231Z","modifiedTime":"2022-06-28T12:20:34.231Z"}},{"node":{"id":"0d0b32f1-75c2-5816-87ae-1b163bfa91a3","name":"dorisbatch-sink","parent":null,"relativePath":"ChunJun连接器/doris/dorisbatch-sink.md","ctime":"2022-06-28T12:20:34.228Z","modifiedTime":"2022-06-28T12:20:34.228Z"}},{"node":{"id":"ba227b55-0c80-5200-ba86-2aac94e86636","name":"hdfs-sink","parent":null,"relativePath":"ChunJun连接器/hdfs/hdfs-sink.md","ctime":"2022-06-28T12:20:34.236Z","modifiedTime":"2022-06-28T12:20:34.236Z"}},{"node":{"id":"f1d51185-de0c-5a7b-8ad0-536cdcfec516","name":"hdfs-source","parent":null,"relativePath":"ChunJun连接器/hdfs/hdfs-source.md","ctime":"2022-06-28T12:20:34.236Z","modifiedTime":"2022-06-28T12:20:34.236Z"}},{"node":{"id":"02f2bf2b-3a65-5813-b06b-b79ef834f46a","name":"hbase-lookup","parent":null,"relativePath":"ChunJun连接器/hbase/hbase-lookup.md","ctime":"2022-06-28T12:20:34.234Z","modifiedTime":"2022-06-28T12:20:34.234Z"}},{"node":{"id":"88da24b7-d05c-5184-a719-f3cd0f0e80a7","name":"dm-source","parent":null,"relativePath":"ChunJun连接器/dm/dm-source.md","ctime":"2022-06-28T12:20:34.226Z","modifiedTime":"2022-06-28T12:20:34.226Z"}},{"node":{"id":"6890377c-3c35-5f05-9b75-88513ddac8ad","name":"hbase-sink","parent":null,"relativePath":"ChunJun连接器/hbase/hbase-sink.md","ctime":"2022-06-28T12:20:34.235Z","modifiedTime":"2022-06-28T12:20:34.235Z"}},{"node":{"id":"87acad36-889a-5389-a9cc-cd96f3d14132","name":"hbase-source","parent":null,"relativePath":"ChunJun连接器/hbase/hbase-source.md","ctime":"2022-06-28T12:20:34.235Z","modifiedTime":"2022-06-28T12:20:34.235Z"}},{"node":{"id":"67a4651a-7554-5849-9f07-1cf8ea79e3e7","name":"dm-sink","parent":null,"relativePath":"ChunJun连接器/dm/dm-sink.md","ctime":"2022-06-28T12:20:34.226Z","modifiedTime":"2022-06-28T12:20:34.226Z"}},{"node":{"id":"41b5d339-0a83-54ab-afbe-fdfa58ba5b87","name":"kafka-sink","parent":null,"relativePath":"ChunJun连接器/kafka/kafka-sink.md","ctime":"2022-06-28T12:20:34.2